<table class="profile" summary="Profile information">
  <tr>
    <td class="main">
      <h1>
        <%= @user.username %>'s jobs.
      </h1>
      <% unless @jobs.empty? %>
      <table class = "jobs" summary="User Jobs">
        <%= render 'user_jobs' %>
      </table>
      <%= will_paginate @jobs %>
      <% end %>
    </td>
    <td class = "sidebar round">
      <%= render 'user_sidebar' %>
    </td>
  </tr>
</table>
